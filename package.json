  "contributors": [
    "Jaroslav Šmolík <grissius@gmail.com>"
  ],
  "main": "./dist/main.js",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "^10.14.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AckeeCZ/unicore"
  },
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "tsc",
