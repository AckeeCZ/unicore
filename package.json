    "docs": "typedoc --options typedoc.json src/lib/*.ts",
    "check": "npm-check -i app -i errors -i config -i index & exit 0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "server-destroy": "^1.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^14.0.1",
    "codecov": "^3.6.5",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.1",
    "npm-check": "^5.9.2",
    "prettier": "^2.0.5",
    "prettier-config-ackee": "^0.0.13",
    "ts-jest": "^26.0.0",
    "tslint-config-ackee": "^0.3.0",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3"
