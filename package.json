    "raven": "^2.6.4",
    "server-destroy": "^1.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.21",
    "@types/request-promise": "^4.1.43",
    "@types/supertest": "^2.0.7",
    "cross-env": "^6.0.3",
    "husky": "^3.0.9",
